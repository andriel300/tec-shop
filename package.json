{
  "name": "@tec-shop/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npx nx run-many --target=serve --all",
    "user-ui": "PORT=3000 npx nx dev user-ui",
    "seller-ui": "PORT=3001 npx nx dev seller-ui",
    "prisma:generate": "npx nx run @tec-shop/auth-schema:generate-types && npx nx run @tec-shop/user-schema:generate-types && npx nx run @tec-shop/seller-schema:generate-types && npx nx run @tec-shop/product-schema:generate-types && npx nx run @tec-shop/analytics-schema:generate-types",
    "prisma:db-push": "npx nx run-many --target=db-push --projects=@tec-shop/auth-schema,@tec-shop/user-schema,@tec-shop/seller-schema,@tec-shop/product-schema,@tec-shop/analytics-schema",
    "prisma:studio:auth": "npx nx run @tec-shop/auth-schema:studio",
    "prisma:studio:user": "npx nx run @tec-shop/user-schema:studio",
    "prisma:studio:seller": "npx nx run @tec-shop/seller-schema:studio",
    "prisma:studio:product": "npx nx run @tec-shop/product-schema:studio",
    "prisma:studio:analytics": "npx nx run @tec-shop/analytics-schema:studio",
    "prisma:studio:all": "npx nx run-many --target=studio --projects=@tec-shop/auth-schema,@tec-shop/user-schema,@tec-shop/seller-schema,@tec-shop/product-schema,@tec-shop/analytics-schema --parallel=5",
    "prisma:format": "npx nx run-many --target=format --projects=@tec-shop/auth-schema,@tec-shop/user-schema,@tec-shop/seller-schema,@tec-shop/product-schema,@tec-shop/analytics-schema",
    "seed:brands": "npx tsx apps/product-service/src/scripts/seed-brands.ts",
    "seed:categories": "npx tsx apps/product-service/src/scripts/seed-categories.ts",
    "seed:products": "npx tsx apps/product-service/src/scripts/seed-products.ts",
    "seed:all": "npm run seed:categories && npm run seed:brands",
    "get-shop-id": "npx tsx apps/product-service/src/scripts/get-shop-id.ts",
    "test": "npx nx run-many --target=test --all",
    "lint": "npx nx run-many --target=lint --all",
    "typecheck": "npx nx run-many --target=typecheck --all"
  },
  "private": true,
